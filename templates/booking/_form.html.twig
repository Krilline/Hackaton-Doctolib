{{ form_start(form) }}
<div class="form-group row">
    <div class="col-10">
        {{ form_widget(form.beginAt, {
            "attr": {"class": "form-control my-2"}
        }) }}
    </div>
</div>
<div class="form-group row">
    <div class="col-10">
        {{ form_widget(form.endAt, {
            "attr": {"class": "form-control my-2"}
        }) }}
    </div>
</div>
<div class="form-group row">
    <div class="col-10">
        {{ form_widget(form.title, {
            "attr": {"class": "form-control my-2"}
        }) }}
    </div>
</div>
<div class="col-12 text-center">
    <button class="btn btn-primary">{{ button_label|default('Agree') }}</button>
</div>
{{ form_end(form) }}